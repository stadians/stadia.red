{
  "cSpell.words": [
    "KHTML",
    "esnext",
    "illufinch",
    "plnadnblnocgoficdcgbbdmcnciagojj",
    "printf",
    "renderable",
    "skus",
    "spidered",
    "spidering",
    "stadiaobsobsobsobsobsobsobstadia",
    "upsert",
    "webext",
    "webextension",
    "woff"
  ],
  "editor.cursorSmoothCaretAnimation": true,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleImplementations": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fit",
  "editor.showFoldingControls": "always",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "cmd": "bash -c 'NODE_VERSION=stable ~/.nvm/nvm-exec yarn build'",
        "isAsync": true,
        "match": ".*"
      }
    ]
  },
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "mixed",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 4000,
  "files.defaultLanguage": " ${activeEditorLanguage}",
  "files.eol": "\n",
  "files.exclude": {
    "*.pid": true,
    "*/CNAME": true,
    ".github": true,
    ".gitignore": true,
    ".vscode": true,
    "LICENSE": true,
    "README.md": true,
    "docs": true,
    "static": true,
    "yarn-error.log": true,
    "yarn.lock": true
  },
  "files.insertFinalNewline": true,
  "files.saveConflictResolution": "overwriteFileOnDisk",
  "files.trimFinalNewlines": true,
  "peacock.affectAccentBorders": true,
  "peacock.affectTabActiveBorder": true,
  "peacock.color": "#d06050",
  "problems.showCurrentInStatus": true,
  "search.exclude": {
    "**/*.code-search": false,
    "**/bower_components": false,
    "**/node_modules": false
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#db8578",
    "activityBar.activeBorder": "#b4ebbb",
    "activityBar.background": "#db8578",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#b4ebbb",
    "activityBarBadge.foreground": "#15202b",
    "editorError.foreground": "#F888",
    "editorGroup.border": "#db8578",
    "minimap.errorHighlight": "#F883",
    "panel.border": "#db8578",
    "sideBar.border": "#db8578",
    "statusBar.background": "#d06050",
    "statusBar.border": "#d06050",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#db8578",
    "tab.activeBorder": "#db8578",
    "titleBar.activeBackground": "#d06050",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.border": "#d06050",
    "titleBar.inactiveBackground": "#d0605099",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "workbench.startupEditor": "readme"
}
