{
  "editor.cursorSmoothCaretAnimation": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "mixed",
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 4000,
  "files.defaultLanguage": " ${activeEditorLanguage}",
  "files.eol": "\n",
  "files.exclude": {
    ".https": true,
    "*.lock": true,
    "*.log": true,
    "*.pid": true,
    "node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "peacock.affectAccentBorders": true,
  "peacock.affectTabActiveBorder": true,
  "peacock.color": "#470e0f",
  "problems.showCurrentInStatus": true,
  "search.exclude": {
    "**/node_modules": false
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#721618",
    "activityBar.activeBorder": "#1c861a",
    "activityBar.background": "#721618",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#1c861a",
    "activityBarBadge.foreground": "#e7e7e7",
    "editorError.foreground": "#F888",
    "editorGroup.border": "#721618",
    "minimap.errorHighlight": "#F883",
    "panel.border": "#721618",
    "sideBar.border": "#721618",
    "statusBar.background": "#470e0f",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#721618",
    "tab.activeBorder": "#721618",
    "titleBar.activeBackground": "#470e0f",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#470e0f99",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
