{
  "cSpell.words": [
    "KHTML",
    "esnext",
    "illufinch",
    "plnadnblnocgoficdcgbbdmcnciagojj",
    "printf",
    "renderable",
    "skus",
    "spidered",
    "spidering",
    "stadiaobsobsobsobsobsobsobstadia",
    "upsert",
    "webext",
    "webextension",
    "woff"
  ],
  "editor.wrappingIndent": "indent",
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "cmd": "bash -c 'NODE_VERSION=stable ~/.nvm/nvm-exec yarn build'",
        "match": ".*"
      }
    ]
  },
  "files.exclude": {
    "*/CNAME": true,
    ".github": true,
    ".vscode": true,
    "LICENSE": true,
    "README.md": true,
    "docs": true,
    "static": true,
    "yarn.lock": true
  },
  "peacock.color": "#d06050",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#db8578",
    "activityBar.activeBorder": "#b4ebbb",
    "activityBar.background": "#db8578",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#b4ebbb",
    "activityBarBadge.foreground": "#15202b",
    "editorError.foreground": "#F884",
    "minimap.errorHighlight": "#F882",
    "statusBar.background": "#d06050",
    "statusBar.border": "#d06050",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#db8578",
    "titleBar.activeBackground": "#d06050",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.border": "#d06050",
    "titleBar.inactiveBackground": "#d0605099",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
