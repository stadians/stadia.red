{
  "dependencies": {
    "webextension-polyfill": "v0.6"
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "v70.0.1",
    "eslint": "^7.0.0",
    "import-sort-cli": "^6.0.0",
    "prettier": "v2.0.5",
    "replace-in-files-cli": "v0.3.1",
    "sort-json": "v2",
    "typescript": "v3.8.3",
    "web-ext": "^4.2.0"
  },
  "engines": {
    "node": "v14.2.0"
  },
  "name": "stadia.observer",
  "prettier": {
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": [
          "*.yml",
          "*.yaml"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "trailingComma": "all"
  },
  "private": true,
  "resolutions": {
    "web-ext/**/shelljs": ">=0.8.4"
  },
  "scripts": {
    "build": "./build",
    "vendor": "git rm -rf node_modules; NODE_ENV=production yarn install; git add -f node_modules/*; NODE_ENV=development yarn install"
  },
  "type": "module",
  "version": "2020.5.11"
}
