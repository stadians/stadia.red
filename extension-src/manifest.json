  "key": "stadiaobsobsobsobsobsobsobstadia",
  "manifest_version": 2,
  "name": "stadians.dev",
  "permissions": ["https://stadia.google.com/*"],
  "version": "2020.9.1",
  "version_name": "2020-09-01"
}
